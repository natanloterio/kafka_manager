{
  "name": "kafka_manager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve-production": "vue-cli-service serve --mode=production",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-docker": "vue-cli-service build && docker build . -t jeffbeagley/kafka_manager",
    "run-docker": "docker run -it -d -e PORT=9500 -p 9500:9500 kafka_manager"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.4.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.5",
    "@fortawesome/free-solid-svg-icons": "^5.4.0",
    "@fortawesome/vue-fontawesome": "^0.1.1",
    "axios": "^0.18.0",
    "bootstrap-vue": "^2.0.0-rc.11",
    "register-service-worker": "^1.0.0",
    "vue": "^2.5.17",
    "vue-json-pretty": "^1.4.1",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-persist": "^2.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-async-to-generator": "^7.2.0",
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-eslint": "^3.0.4",
    "@vue/cli-plugin-pwa": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "vue-template-compiler": "^2.5.17"
  }
}
